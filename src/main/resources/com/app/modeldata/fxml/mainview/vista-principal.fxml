<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.controllers.modeldata.ModelDataAppController">

    <top>
        <HBox fx:id="topBar" prefHeight="60.0" style="-fx-background-color: #197CA1;" />
    </top>

    <left>
        <VBox fx:id="sideBar" prefWidth="200" style="-fx-background-color: #2b2b2b;" />
    </left>

    <center>
        <AnchorPane fx:id="contentPane" />
    </center>
</BorderPane>
