<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.controllers.panels.menubar.itemsanalizer.FrequencyAnalizerController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="15.0">
         <children>
            <HBox prefWidth="200.0">
               <children>
                  <Label text="Frecuencias">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Variables">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
                  <ComboBox prefHeight="25.0" prefWidth="97.0" />
               </children>
            </HBox>
            <HBox prefHeight="180.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <VBox minHeight="180.0" prefWidth="300.0">
                     <children>
                        <HBox maxWidth="300.0" prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #d9d9d9;" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="180.0" text="Estadísticos" />
                        <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="180.0" text="Gráficos" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Aceptar" />
                  <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Restablecer" />
                  <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Cancelar" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
